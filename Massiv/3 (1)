
public class Main {
    public static void main(String[] args) {
        int x = 0;
        int[] masiv = {2, 11, 6, 2, 10, 3, 10, 5};
        for (int i = 0; i < masiv.length; i++) {
            if (masiv[i] % 2 == 0) x = masiv[i] + x;

        }System.out.println("Сумма четных чисел " + x);
    }
}